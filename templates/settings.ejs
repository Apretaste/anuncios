<div class="row">
	<div class="col s12">
		<ul class="tabs tabs-fixed-width">
			<li class="tab"><a href="#" class="active">Anuncios</a></li>
			<li class="tab"><a href="#" onclick="apretaste.send({command: 'ADS SETTINGS'})">Ajustes</a></li> 
		</ul>
	</div>
</div>

<div class="row">
	<ul class="collection">
		<% ads.forEach(function(ad){ %>
			<li class="collection-item avatar">
				<img src="{{APP_IMAGE_PATH}}<%= ad.icon %>" alt="<%= ad.icon %>" class="circle">
				<span class="title green-text"><%= ad.title %></span>
				<p class="stars" data-value="<%= ad.ctr %>"></p>
				<p class="subtitle"><%= ad.subtitle %></p>
			</li>
		<% }) %>
	</ul>
</div>